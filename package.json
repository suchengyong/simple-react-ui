{
  "private": false,
  "version": "0.0.1",
  "name": "suchengyong-react-simple-ui",
  "description": "一个简洁风格的react组件库",
  "keywords": [
    "components",
    "design",
    "react",
    "react-component",
    "typescript",
    "ui"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/simple-react.js",
  "author": "suchengyong",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "bugs": {
    "url": "https://github.com/suchengyong/simple-react-ui.git/issues"
  },
  "homepage": "https://github.com/suchengyong/simple-react-ui.git#readme",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "^1.4.22",
    "@umijs/test": "^3.0.5",
    "classnames": "^2.2.6",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "dumi": "^1.0.24",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2"
  }
}
